{
	# detectors is a non cumulative structured option.
	#
	# -strict non passed: the last instance of this option will overwrite any previous instances
	# -strict passed: exit if a duplicate option is found
	#
	# Corresponding command line example:
	#
	# -detector={target: experiments/clas12/rtpc; factory: TEXT; variation: experiments/clas12/rtpc;}
	#
	# as an example we use carriage return to organize the fields
	"gdetector": {
		"detector": "experiments/clas12/rtpc",
		"factory": "TEXT",
	}
}
